{
  "name": "rihardsgrig/xml_processor",
  "description": "Xml file processor",
  "type": "project",
  "license": "proprietary",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "monolog/monolog": "^3.2",
    "symfony/console": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Xml\\Processor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xml\\Processor\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "bin": [
    
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
